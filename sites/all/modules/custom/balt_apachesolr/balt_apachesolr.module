/**
 * Implements hook_apachesolr_search_result().
 */
function balt_apachesolr_apachesolr_search_result_alter(&$doc, $extra, $query) {
  // We aren't getting entity_type from nutch.
  if (!isset($doc->entity_type)) {
    $doc->entity_type = '';
  }
}
